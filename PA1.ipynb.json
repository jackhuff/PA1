   "source": [
    "num1=input('Enter a floating point number: ')\n",
    "num2=input('Enter an integer: ')\n",
    "sum= float(num1) + int(num2)\n",
    "print(\"The sum is: \",sum)\n",
    "dif= float(num1) - int(num2)\n",
    "print(\"The difference is: \", dif)\n",
    "product= float(num1) * int(num2)\n",
    "print(\"The product is: \", product)\n",
    "quotient= float(num1) // int(num2)\n",
    "print(\"The quotient is: \", quotient)\n",
    "print(\"The type for the result is\", (type(num1)))"
   ]
